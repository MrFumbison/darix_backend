    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="" />
    <meta name="keywords" content="">
    <meta name="author" content="Phoenixcoded" />
    <!-- Favicon icon -->
    <link rel="icon" href="/images/favicon.ico" type="image/x-icon">

    <!-- vendor css -->
    <link rel="stylesheet" href="/css/style.css">
    
    

</head>
<body class="">
	<!-- [ navigation menu ] start -->

    
	<nav class="pcoded-navbar  ">
		<div class="navbar-wrapper  ">
			<div class="navbar-content scroll-div " >				
				<ul class="nav pcoded-inner-navbar ">
				
					<li class="nav-item">
					    <a href="/dashboard" class="nav-link "><span class="pcoded-micon"><i class="feather icon-home"></i></span><span class="pcoded-mtext">Dashboard</span></a>			

				</ul>
				
			</div>
		</div>
	</nav>
	<!-- [ navigation menu ] end -->
	<!-- [ Header ] start -->
	<header class="navbar pcoded-header navbar-expand-lg navbar-light header-dark">	
        <div class="m-header">
            <a href="/dashboard" class="b-brand">
                <!-- ========   change your logo hear   ============ -->
                <img src="/images/logo.png" alt="" class="logo">
                <img src="/images/logo-icon.png" alt="" class="logo-thumb">
            </a>
        </div>
        <div class="collapse navbar-collapse">          
            <ul class="navbar-nav ml-auto">              
                <li>
                    <div class="dropdown drp-user">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="feather icon-user"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right profile-notification">
                            <div class="pro-head">
                                <img src="/images/user/user_1.png" class="img-radius" alt="User-Profile-Image">
                                <span><%= user.name %></span>
                               
                            </div>
                            <ul class="pro-body">                    
                                <li><a href="/api/auth/logout" class="dropdown-item"><i class="feather icon-lock"></i> Sign Out</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
            </ul>
        </div>    
</header>
<!-- [ Header ] end -->
	

<!-- [ Main Content ] start -->
<div class="pcoded-main-container">
    <div class="pcoded-content">
        <!-- [ breadcrumb ] start -->
        <div class="page-header">
            <div class="page-block">
                <div class="row align-items-center">
                    <div class="col-md-12">
                        <div class="page-header-title">
                            <h5 class="m-b-10">List of Comments</h5>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
        <!-- [ breadcrumb ] end -->
        <!-- [ Main Content ] start -->
        <div class="row">
            
           
           

            <!-- prject ,team member start -->
            <div class="col-xl-12 col-md-12">
                <div class="card table-card">
                    
                    <div class="card-body p-0">
                        <div class="table-responsive">
                            <table class="table table-hover mb-0">
                                <thead>
                                  <tr>
                                    <th>Company Name</th>
                                    <th>Your Email</th>
                                    <th>Phone Number</th>
                                    <th class="text-right">Submitted Date</th>
                                    <th class="text-right">Action</th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <% messages.forEach(message => { %>
                                  <tr>
                                    <td>
                                      <div class="d-inline-block align-middle">
                                        <div class="d-inline-block">
                                          <h6><%= message.companyName %></h6>
                                        </div>
                                      </div>
                                    </td>
                                    <td><%= message.email %></td>
                                    <td><%= message.contactNumber %></td>
                                    <td class="text-right"><%= new Date(message.createdAt).toLocaleDateString() %></td>
                                    <td class="text-right">
                                      <a href="/api/getmessage/<%= message.id %>" class="btn btn-primary">View</a>
                                    </td>
                                  </tr>
                                  <% }) %>
                                </tbody>
                              </table>
                              
                        </div>
                    </div>
                </div>
            </div>
           
            <!-- prject ,team member start -->
           
        </div>
        <!-- [ Main Content ] end -->
    </div>
</div>
<!-- [ Main Content ] end -->
